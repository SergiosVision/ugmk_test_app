{
  "name": "ugmk-test-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "json-server": "json-server --watch public/json/products.json --port 3001 --delay 1000",
    "dockerize": "docker build -t ugmk_test_app .",
    "start-container": "docker container run -it -p 3000:3000 --name ugmk_test_app --rm ugmk_test_app"
  },
  "dependencies": {
    "highcharts": "10.3.3",
    "highcharts-react-official": "3.2.0",
    "json-server": "0.17.3",
    "lodash": "4.17.21",
    "mobx": "6.9.0",
    "mobx-react-lite": "3.4.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-loading-skeleton": "3.2.1",
    "react-router-dom": "6.10.0",
    "yup": "1.1.1"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "4.1.1",
    "@types/lodash": "4.14.194",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/yup": "0.32.0",
    "@typescript-eslint/eslint-plugin": "5.57.1",
    "@typescript-eslint/parser": "5.57.1",
    "@vitejs/plugin-react": "4.0.0",
    "autoprefixer": "10.4.14",
    "cssnano": "6.0.0",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.3.4",
    "postcss": "8.4.23",
    "prettier": "2.8.8",
    "sass": "1.62.1",
    "tailwindcss": "3.3.2",
    "terser": "5.17.1",
    "typescript": "5.0.2",
    "vite": "4.3.2"
  }
}
